<h1>Your Dashboard</h1>
<button id="new-post-btn">Create New Post</button>
<form id="new-post-form" class="hidden">
  <input type="text" id="post-title" placeholder="Title">
  <textarea id="post-content" placeholder="Content"></textarea>
  <button type="submit">Submit</button>
</form>
<div id="dashboard-posts">
  {{#each user.posts}}
    <div class="post-card">
      <h2>{{this.title}}</h2>
      <p>{{this.content}}</p>
      <button class="edit-post-btn" data-id="{{this.id}}">Edit</button>
      <button class="delete-post-btn" data-id="{{this.id}}">Delete</button>
    </div>
  {{/each}}
</div>